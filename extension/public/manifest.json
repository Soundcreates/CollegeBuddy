{
  "manifest_version": 3,
  "name": "Somaiya Extension",
  "version": "1.0.0",
  "description": "Somaiya College Extension",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "alarms"
  ],
  "host_permissions": [
    "http://localhost:8080/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Somaiya Extension"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:8080/*"
    ]
  }
}
